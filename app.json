{
  "expo": {
    "name": "FriendsList",
    "description": "Example App demonstrating navigation, lists and other things.",
    "slug": "FriendsList",
    "privacy": "public",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "package": "de.deinedomain.deineapp"
    },
    "ios": {
      "bundleIdentifier": "de.deinedomain.deineapp",
      "supportsTablet": true
    }
  }
}
